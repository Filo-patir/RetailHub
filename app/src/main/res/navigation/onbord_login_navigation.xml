<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_lauout"
    app:startDestination="@id/signUpFragment">
    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.iti4.retailhub.features.loginandsignup.view.SignUpFragment"
        android:label="fragment_sign_up"
        tools:layout="@layout/fragment_sign_up" >
        <action
            android:id="@+id/action_signUpFragment_to_loginInFragment"
            app:destination="@id/loginInFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginInFragment"
        android:name="com.iti4.retailhub.features.loginandsignup.view.LoginInFragment"
        android:label="fragment_login_in"
        tools:layout="@layout/fragment_login_in" />
</navigation>